{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID": "@walletconnect_project_id",
    "NEXT_PUBLIC_PIT_TIPPING_ADDRESS": "@pit_tipping_address",
    "NEXT_PUBLIC_FARCASTER_ORACLE_ADDRESS": "@farcaster_oracle_address"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID": "@walletconnect_project_id",
      "NEXT_PUBLIC_PIT_TIPPING_ADDRESS": "@pit_tipping_address",
      "NEXT_PUBLIC_FARCASTER_ORACLE_ADDRESS": "@farcaster_oracle_address"
    }
  }
}