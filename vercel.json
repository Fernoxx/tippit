{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_NEYNAR_API_KEY": "@neynar_api_key",
    "NEYNAR_API_KEY": "@neynar_api_key",
    "WEBHOOK_SECRET": "@webhook_secret",
    "NEXT_PUBLIC_PIT_TIPPING_ADDRESS": "@pit_tipping_address",
    "NEXT_PUBLIC_FARCASTER_ORACLE_ADDRESS": "@farcaster_oracle_address",
    "INTERNAL_API_KEY": "@internal_api_key"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_NEYNAR_API_KEY": "@neynar_api_key",
      "NEXT_PUBLIC_PIT_TIPPING_ADDRESS": "@pit_tipping_address",
      "NEXT_PUBLIC_FARCASTER_ORACLE_ADDRESS": "@farcaster_oracle_address"
    }
  }
}