{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "public": false,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "@backend_url",
    "NEXT_PUBLIC_BACKEND_WALLET_ADDRESS": "@backend_wallet_address",
    "WEBHOOK_SECRET": "@webhook_secret"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BACKEND_URL": "@backend_url",
      "NEXT_PUBLIC_BACKEND_WALLET_ADDRESS": "@backend_wallet_address",
      "WEBHOOK_SECRET": "@webhook_secret"
    }
  }
}